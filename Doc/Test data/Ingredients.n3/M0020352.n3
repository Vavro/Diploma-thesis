<http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000008581> a ns1:PhysiologicEffect ;
    ns1:title "Decreased Distal Tubule H+ Excretion"@en .

<http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000008583> a ns1:PhysiologicEffect ;
    ns1:title "Decreased Distal Tubule K+ Excretion"@en .

<http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000009279> a ns1:PhysiologicEffect ;
    ns1:title "Increased Distal Tubule Na+ Excretion"@en .

<http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000178477> a ns1:PhysiologicEffect ;
    ns1:title "Decreased Blood Pressure"@en .

