<http://linked.opendata.cz/resource/ndfrt/disease/N0000000564> a ns1:DiseaseOrFinding ;
    ns1:description "Tumors or cancer of the URINARY BLADDER.     "@en ;
    ns1:title "Močový měchýř - nádory"@cs,
