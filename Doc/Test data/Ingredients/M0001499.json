ontology/drug-encyclopedia/description" : [ { "@value" : "Inflammation of the MIDDLE EAR including the AUDITORY OSSICLES and the EUSTACHIAN TUBE.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000009925",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PhysiologicEffect" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Unknown Physiological Effect" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000009915",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MechanismOfAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Unknown Cellular or Molecular Interaction" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/ingredient/M0001499",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/Ingredient" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Antipyrin" , "@language" : "cs" } ,        { "@value" : "Antipyrine" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPharmacologicalAction" : [ "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0001335" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMedicinalProduct" : [ "http://linked.opendata.cz/resource/sukl/medicinal-product/OTIPAX" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "An analgesic and antipyretic that has been given by mouth and as ear drops. Antipyrine is often used in testing the effects of other drugs or diseases on drug-metabolizing enzymes in the liver. (From Martindale, The Extra Pharmacopoeia, 30th ed, p29)     " , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMechanismOfAction" : [ "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000009915" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/mayTreat" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000002266" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000002278" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/contraindicatedWith" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000000999" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPhysiologicEffect" : [ "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000009925" ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0001335",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Anti-inflammatory agents, non-steroidal" , "@language" : "en" } ,        { "@value" : "Antiflogistika nesteroidn\u00ED" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Anti-inflammatory agents that are not steroids. In addition to anti-inflammatory actions, they have analgesic, antipyretic, and platelet-inhibitory actions. They are used primarily in the treatment of chronic arthritic conditions and certain soft tissue disorders associated with pain and inflammation. They act by blocking the synthesis of prostaglandins by inhibiting cyclooxygenase, which converts arachidonic acid to cyclic endoperoxides, precursors of prostaglandins. Inhibition of prostaglandin synthesis accounts for their analgesic, antipyretic, and platelet-inhibitory actions; other mechanisms may contribute to their anti-inflammatory effects. Certain NSAIDs also may inhibit lipoxygenase enzymes or TYPE C PHOSPHOLIPASES or may modulate T-cell function. (AMA Drug Evaluations Annual, 1994, p 1814-5)     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/sukl/medicinal-product/OTIPAX",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MedicinalProduct" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "OTIPAX" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasATCConcept" : [ "http://linked.opendata.cz/resource/ATC/S02DA30" ] }] }