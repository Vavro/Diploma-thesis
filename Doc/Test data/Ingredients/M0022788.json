{ "@graph": [    { "@id": "http://linked.opendata.cz/resource/ATC/S01XA02",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/ATCConcept" ] ,      "http://www.w3.org/2004/02/skos/core#prefLabel" : [ { "@value" : "Retinol" , "@language" : "en" } ,        { "@value" : "Retinol" , "@language" : "cs" } ] ,      "http://www.w3.org/2004/02/skos/core#notation" : [ { "@value" : "S01XA02" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000999",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Drug hypersensitivity" , "@language" : "en" } ,        { "@value" : "L\u00E9kov\u00E1 alergie" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Immunologically mediated adverse reactions to medicinal substances used legally or illegally.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002396",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Poruchy fotosenzitivity" , "@language" : "cs" } ,        { "@value" : "Photosensitivity disorders" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Abnormal responses to sunlight or artificial light due to extreme reactivity of light-absorbing molecules in tissues. It refers almost exclusively to skin photosensitivity, including sunburn, reactions due to repeated prolonged exposure in the absence of photosensitizing factors, and reactions requiring photosensitizing factors such as photosensitizing agents and certain diseases. With restricted reference to skin tissue, it does not include photosensitivity of the eye to light, as in photophobia or photosensitive epilepsy.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000939",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Dermatitis" , "@language" : "en" } ,        { "@value" : "Dermatitida" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Any inflammation of the skin.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000003131",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Nedostatek vitaminu a" , "@language" : "cs" } ,        { "@value" : "Vitamin a deficiency" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A nutritional condition produced by a deficiency of VITAMIN A in the diet, characterized by NIGHT BLINDNESS and other ocular manifestations such as dryness of the conjunctiva and later of the cornea (XEROPHTHALMIA). Vitamin A deficiency is a very common problem worldwide, particularly in developing countries as a consequence of famine or shortages of vitamin A-rich foods. In the United States it is found among the urban poor, the elderly, alcoholics, and patients with malabsorption. (From Cecil Textbook of Medicine, 19th ed, p1179)     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000003178",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Xerophthalmia" , "@language" : "en" } ,        { "@value" : "Xeroftalmie" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Dryness of the eye surfaces caused by deficiency of tears or conjunctival secretions. It may be associated with vitamin A deficiency, trauma, or any condition in which the eyelids do not close completely.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002198",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "No\u010Dn\u00ED slepota" , "@language" : "cs" } ,        { "@value" : "Night blindness" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Failure or imperfection of vision at night or in dim light, with good vision only on bright days. (Dorland, 27th ed)     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000008337",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PhysiologicEffect" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Cellular Activity Alteration" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000000170",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MechanismOfAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Free Radical Scavenging Activity" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000000163",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MechanismOfAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Enzyme Interactions" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/ingredient/M0022788",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/Ingredient" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Vitamin a" , "@language" : "en" } ,        { "@value" : "Vitamin a" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPharmacologicalAction" : [ "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0022811" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMedicinalProduct" : [ "http://linked.opendata.cz/resource/sukl/medicinal-product/VITAMIN-A-POS" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Retinol and derivatives of retinol that play an essential role in metabolic functioning of the retina, the growth of and differentiation of epithelial tissue, the growth of bone, reproduction, and the immune response. Dietary vitamin A is derived from a variety of CAROTENOIDS found in plants. It is enriched in the liver, egg yolks, and the fat component of dairy products.     " , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMechanismOfAction" : [ "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000000163" ,        "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000000170" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/mayTreat" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000002396" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000002198" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000000939" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000003131" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000003178" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/mayPrevent" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000003131" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/contraindicatedWith" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000000999" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPhysiologicEffect" : [ "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000008337" ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0022811",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Vitamins" , "@language" : "en" } ,        { "@value" : "Vitaminy" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Organic substances that are required in small amounts for maintenance and growth, but which cannot be manufactured by the human body.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/sukl/medicinal-product/VITAMIN-A-POS",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MedicinalProduct" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "VITAMIN A-POS" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasATCConcept" : [ "http://linked.opendata.cz/resource/ATC/S01XA02" ] }] }