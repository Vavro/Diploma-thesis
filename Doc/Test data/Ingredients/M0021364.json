] } ,    { "@id": "http://linked.opendata.cz/resource/sukl/medicinal-product/THIOPENTAL-VUAB-INJ--PLV--SOL--1-0-G",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MedicinalProduct" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "THIOPENTAL VUAB INJ. PLV. SOL. 1,0 G" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasATCConcept" : [ "http://linked.opendata.cz/resource/ATC/N01AF03" ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0001382",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Antikonvulz\u00EDva" , "@language" : "cs" } ,        { "@value" : "Anticonvulsants" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Drugs used to prevent SEIZURES or reduce their severity.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0010885",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Hypnotics and sedatives" , "@language" : "en" } ,        { "@value" : "Hypnotika a sedativa" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Drugs used to induce drowsiness or sleep or to reduce psychological excitement or anxiety.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0027999",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Anestetika intraven\u00F3zn\u00ED" , "@language" : "cs" } ,        { "@value" : "Anesthetics, intravenous" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Ultrashort-acting anesthetics that are used for induction. Loss of consciousness is rapid and induction is pleasant, but there is no muscle relaxation and reflexes frequently are not reduced adequately. Repeated administration results in accumulation and prolongs the recovery time. Since these agents have little if any analgesic activity, they are seldom used alone except in brief minor procedures. (From AMA Drug Evaluations Annual, 1994, p174)     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0028093",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Gaba modul\u00E1tory" , "@language" : "cs" } ,        { "@value" : "Gaba modulators" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Substances that do not act as agonists or antagonists but do affect the GAMMA-AMINOBUTYRIC ACID receptor-ionophore complex. GABA-A receptors (RECEPTORS, GABA-A) appear to have at least three allosteric sites at which modulators act: a site at which BENZODIAZEPINES act by increasing the opening frequency of GAMMA-AMINOBUTYRIC ACID-activated chloride channels; a site at which BARBITURATES act to prolong the duration of channel opening; and a site at which some steroids may act. GENERAL ANESTHETICS probably act at least partly by potentiating GABAergic responses, but they are not included here.     " , "@language" : "en" } ] }] }