} ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002114",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Nausea" , "@language" : "en" } ,        { "@value" : "Nauzea" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "An unpleasant sensation in the stomach usually accompanied by the urge to vomit. Common causes are early pregnancy, sea and motion sickness, emotional stress, intense pain, food poisoning, and various enteroviruses.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002031",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Kinet\u00F3zy" , "@language" : "cs" } ,        { "@value" : "Motion sickness" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Disorder caused by motion, as sea sickness, train sickness, car sickness, air sickness, or SPACE MOTION SICKNESS. It may include nausea, vomiting and dizziness.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000003121",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Vertigo" , "@language" : "en" } ,        { "@value" : "Vertigo" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "An illusion of movement, either of the external world revolving around the individual or of the individual revolving in space. Vertigo may be associated with disorders of the inner ear (EAR, INNER); VESTIBULAR NERVE; BRAINSTEM; or CEREBRAL CORTEX. Lesions in the TEMPORAL LOBE and PARIETAL LOBE may be associated with FOCAL SEIZURES that may feature vertigo as an ictal manifestation. (From Adams et al., Principles of Neurology, 6th ed, pp300-1)     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000008657",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PhysiologicEffect" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Decreased Histamine Activity" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000000190",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MechanismOfAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Histamine H1 Receptor Antagonists" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/ingredient/M0006438",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/Ingredient" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Dimenhydrin\u00E1t" , "@language" : "cs" } ,        { "@value" : "Dimenhydrinate" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPharmacologicalAction" : [ "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0010395" ,        "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0001391" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMedicinalProduct" : [ "http://linked.opendata.cz/resource/sukl/medicinal-product/ARLEVERT" ,        "http://linked.opendata.cz/resource/sukl/medicinal-product/TRAVEL--GUM-20-MG" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A drug combination that contains DIPHENHYDRAMINE and THEOPHYLLINE. It is used for treating vertigo, motion sickness, and nausea associated with pregnancy. It is not effective in the treatment of nausea associated with cancer chemotherapy.     " , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMechanismOfAction" : [ "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000000190" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPregnancyCategory" : [ "http://linked.opendata.cz/resource/fda-spl/pregnancy-category/B" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/mayTreat" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000002114" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000003141" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000003121" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000002031" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/contraindicatedWith" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000000999" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPhysiologicEffect" : [ "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000008657" ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0001391",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Antiemetika" , "@language" : "cs" } ,        { "@value" : "Antiemetics" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Drugs used to prevent NAUSEA or VOMITING.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0010395",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Histamin h1 - antagonist\u00E9" , "@language" : "cs" } ,        { "@value" : "Histamine h1 antagonists" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Drugs that selectively bind to but do not activate histamine H1 receptors, thereby blocking the actions of endogenous histamine. Included here are the classical antihistaminics that antagonize or prevent the action of histamine mainly in immediate hypersensitivity. They act in the bronchi, capillaries, and some other smooth muscles, and are used to prevent or allay motion sickness, seasonal rhinitis, and allergic dermatitis and to induce somnolence. The effects of blocking central nervous system H1 receptors are not as well understood.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/sukl/medicinal-product/ARLEVERT",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MedicinalProduct" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "ARLEVERT" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasATCConcept" : [ "http://linked.opendata.cz/resource/ATC/N07CA52" ] } ,    { "@id": "http://linked.opendata.cz/resource/sukl/medicinal-product/TRAVEL--GUM-20-MG",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MedicinalProduct" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "TRAVEL- GUM 20 MG" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasATCConcept" : [ "http://linked.opendata.cz/resource/ATC/A04AD" ] }] }