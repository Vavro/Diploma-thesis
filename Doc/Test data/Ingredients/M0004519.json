{ "@graph": [    { "@id": "http://linked.opendata.cz/resource/ATC/V03AF03",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/ATCConcept" ] ,      "http://www.w3.org/2004/02/skos/core#prefLabel" : [ { "@value" : "Kalcium-folin\u00E1t" , "@language" : "cs" } ,        { "@value" : "Calcium folinate" , "@language" : "en" } ] ,      "http://www.w3.org/2004/02/skos/core#notation" : [ { "@value" : "V03AF03" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000999",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Drug hypersensitivity" , "@language" : "en" } ,        { "@value" : "L\u00E9kov\u00E1 alergie" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Immunologically mediated adverse reactions to medicinal substances used legally or illegally.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000384",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Anemie" , "@language" : "cs" } ,        { "@value" : "Anemia" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A reduction in the number of circulating erythrocytes or in the quantity of hemoglobin.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000393",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Anemia, megaloblastic" , "@language" : "en" } ,        { "@value" : "Megaloblastov\u00E1 an\u00E9mie" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A disorder characterized by the presence of ANEMIA, abnormally large red blood cells (megalocytes or macrocytes), and MEGALOBLASTS.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000001267",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Nedostatek kyseliny listov\u00E9" , "@language" : "cs" } ,        { "@value" : "Folic acid deficiency" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A nutritional condition produced by a deficiency of FOLIC ACID in the diet. Many plant and animal tissues contain folic acid, abundant in green leafy vegetables, yeast, liver, and mushrooms but destroyed by long-term cooking. Alcohol interferes with its intermediate metabolism and absorption. Folic acid deficiency may develop in long-term anticonvulsant therapy or with use of oral contraceptives. This deficiency causes anemia, macrocytic anemia, and megaloblastic anemia. It is indistinguishable from vitamin B 12 deficiency in peripheral blood and bone marrow findings, but the neurologic lesions seen in B 12 deficiency do not occur. (Merck Manual, 16th ed)     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000396",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Pernici\u00F3zn\u00ED an\u00E9mie" , "@language" : "cs" } ,        { "@value" : "Anemia, pernicious" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A megaloblastic anemia occurring in children but more commonly in later life, characterized by histamine-fast achlorhydria, in which the laboratory and clinical manifestations are based on malabsorption of vitamin B 12 due to a failure of the gastric mucosa to secrete adequate and potent intrinsic factor. (Dorland, 27th ed)     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000001000",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "L\u00E9ky - toxicita" , "@language" : "cs" } ,        { "@value" : "Drug toxicity" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Manifestations of the adverse effects of drugs administered therapeutically or in the course of diagnostic techniques. It does not include accidental or intentional poisoning for which specific headings are available.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000001815",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Leukemie" , "@language" : "cs" } ,        { "@value" : "Leukemia" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A progressive, malignant disease of the blood-forming organs, characterized by distorted proliferation and development of leukocytes and their precursors in the blood and bone marrow. Leukemias were originally termed acute or chronic based on life expectancy but now are classified according to cellular maturity. Acute leukemias consist of predominately immature cells; chronic leukemias are composed of more mature cells. (From The Merck Manual, 2006)     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000001416",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Head and neck neoplasms" , "@language" : "en" } ,        { "@value" : "N\u00E1dory hlavy a krku" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Soft tissue tumors or cancer arising from the mucosal surfaces of the LIP; oral cavity; PHARYNX; LARYNX; and cervical esophagus. Other sites included are the NOSE and PARANASAL SINUSES; SALIVARY GLANDS; THYROID GLAND and PARATHYROID GLANDS; and MELANOMA and non-melanoma skin cancers of the head and neck. (from Holland et al., Cancer Medicine, 4th ed, p1651)     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000664",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Carcinoma" , "@language" : "en" } ,        { "@value" : "Karcinom" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A malignant neoplasm made up of epithelial cells tending to infiltrate the surrounding tissues and give rise to metastases. It is a histological type of neoplasm but is often wrongly used as a synonym for \"cancer.\" (From Dorland, 27th ed)     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000001905",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Lymphoma" , "@language" : "en" } ,        { "@value" : "Lymfom" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A general term for various neoplastic diseases of the lymphoid tissue.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.o