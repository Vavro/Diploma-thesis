{ "@graph": [    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/ingredient/M0000973",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/Ingredient" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Amiodaron" , "@language" : "cs" } ,        { "@value" : "Amiodarone" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPharmacologicalAction" : [ "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0007520" ,        "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0022559" ,        "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0001326" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMedicinalProduct" : [ "http://linked.opendata.cz/resource/sukl/medicinal-product/CORDARONE" ,        "http://linked.opendata.cz/resource/sukl/medicinal-product/AMIODARON-MYLAN-200-MG" ,        "http://linked.opendata.cz/resource/sukl/medicinal-product/SEDACORON" ,        "http://linked.opendata.cz/resource/sukl/medicinal-product/AMIOHEXAL-200" ,        "http://linked.opendata.cz/resource/sukl/medicinal-product/AMIOKORDIN" ,        "http://linked.opendata.cz/resource/sukl/medicinal-product/RIVODARON-200" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "An antianginal and antiarrhythmic drug. It increases the duration of ventricular and atrial muscle action by inhibiting Na,K-activated myocardial adenosine triphosphatase. There is a resulting decrease in heart rate and in vascular resistance.     " , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMechanismOfAction" : [ "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000000161" ,        "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000000069" ,        "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0