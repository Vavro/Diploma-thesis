{ "@graph": [    { "@id": "http://linked.opendata.cz/resource/ATC/L01XX23",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/ATCConcept" ] ,      "http://www.w3.org/2004/02/skos/core#prefLabel" : [ { "@value" : "Mitotane" , "@language" : "en" } ,        { "@value" : "Mitotan" , "@language" : "cs" } ] ,      "http://www.w3.org/2004/02/skos/core#notation" : [ { "@value" : "L01XX23" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000999",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Drug hypersensitivity" , "@language" : "en" } ,        { "@value" : "L\u00E9kov\u00E1 alergie" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Immunologically mediated adverse reactions to medicinal substances used legally or illegally.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000883",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Cushing\u016Fv syndrom" , "@language" : "cs" } ,        { "@value" : "Cushing syndrome" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A condition caused by prolonged exposure to excess levels of cortisol (HYDROCORTISONE) or other GLUCOCORTICOIDS from endogenous or exogenous sources. It is characterized by upper body OBESITY; OSTEOPOROSIS; HYPERTENSION; DIABETES MELLITUS; HIRSUTISM; AMENORRHEA; and excess body fluid. Endogenous Cushing syndrome or spontaneous hypercortisolism is divided into two groups, those due to an excess of ADRENOCORTICOTROPIN and those that are ACTH-independent.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000323",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Adrenokortik\u00E1ln\u00ED n\u00E1dory" , "@language" : "cs" } ,        { "@value" : "Adrenal cortex neoplasms" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Tumors or cancers of the ADRENAL CORTEX.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000008645",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PhysiologicEffect" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Decreased Glucocorticoid Secretion" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000009915",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MechanismOfAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Unknown Cellular or Molecular Interaction" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/ingredient/M0013951",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/Ingredient" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Mitotane" , "@language" : "en" } ,        { "@value" : "Mitotan" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPharmacologicalAction" : [ "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0028300" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMedicinalProduct" : [ "http://linked.opendata.cz/resource/sukl/medicinal-product/LYSODREN" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : " A derivative of the insecticide  DICHLORODIPHENYLDICHLOROETHANE that specifically inhibits cells of the adrenal cortex and their production of hormones. It is used to treat adrenocortical tumors and causes CNS damage, but no bone marrow depression.     " , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMechanismOfAction" : [ "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000009915" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPregnancyCategory" : [ "http://linked.opendata.cz/resource/fda-spl/pregnancy-category/C" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/mayTreat" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000000323" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000000883" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/contraindicatedWith" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000000999" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPhysiologicEffect" : [ "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000008645" ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0028300",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Antineoplastic agents, hormonal" , "@language" : "en" } ,        { "@value" : "Antitumor\u00F3zn\u00ED l\u00E1tky hormon\u00E1ln\u00ED" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Antineoplastic agents that are used to treat hormone-sensitive tumors. Hormone-sensitive tumors may be hormone-dependent, hormone-responsive, or both. A hormone-dependent tumor regresses on removal of the hormonal stimulus, by surgery or pharmacological block. Hormone-responsive tumors may regress when pharmacologic amounts of hormones are administered regardless of whether previous signs of hormone sensitivity were observed. The major hormone-responsive cancers include carcinomas of the breast, prostate, and endometrium; lymphomas; and certain leukemias. (From AMA Drug Evaluations Annual 1994, p2079)     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/sukl/medicinal-product/LYSODREN",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MedicinalProduct" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "LYSODREN" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasATCConcept" : [ "http://linked.opendata.cz/resource/ATC/L01XX23" ] }] }