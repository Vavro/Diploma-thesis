e" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Infarkt myokardu" , "@language" : "cs" } ,        { "@value" : "Myocardial infarction" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "NECROSIS of the MYOCARDIUM caused by an obstruction of the blood supply to the heart (CORONARY CIRCULATION).     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000004102",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Intracranial hemorrhages" , "@language" : "en" } ,        { "@value" : "Intrakrani\u00E1ln\u00ED hemoragie" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Bleeding within the SKULL, including hemorrhages in the brain and the three membranes of MENINGES. The escape of blood often leads to the formation of HEMATOMA in the cranial epidural, subdural, and subarachnoid spaces.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002935",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Thrombophlebitis" , "@language" : "en" } ,        { "@value" : "Tromboflebitida" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Inflammation of a vein associated with a blood clot (THROMBUS).     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002934",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Tromboembolie" , "@language" : "cs" } ,        { "@value" : "Thromboembolism" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Obstruction of a blood vessel (embolism) by a blood clot (THROMBUS) in the blood stream.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000858",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Coronary thrombosis" , "@language" : "en" } ,        { "@value" : "Koron\u00E1rn\u00ED tromb\u00F3za" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Coagulation of blood in any of the CORONARY VESSELS. The presence of a blood clot (THROMBUS) often leads to MYOCARDIAL INFARCTION.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002932",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Thrombocytopenia" , "@language" : "en" } ,        { "@value" : "Trombocytopenie" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A subnormal level of BLOOD PLATELETS.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000985",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Diseminovan\u00E1 intravaskul\u00E1rn\u00ED koagulace" , "@language" : "cs" } ,        { "@value" : "Disseminated intravascular coagulation" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A disorder characterized by procoagulant substances entering the general circulation causing a systemic thrombotic process. The activation of the clotting mechanism may arise from any of a number of disorders. A majority of the patients manifest skin lesions, sometimes leading to PURPURA FULMINANS.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000004074",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Venous thrombosis" , "@language" : "en" } ,        { "@value" : "\u017Diln\u00ED tromb\u00F3za" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "The formation or presence of a blood clot (THROMBUS) within a vein.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002541",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Plicn\u00ED embolie" , "@language" : "cs" } ,        { "@value" : "Pulmonary embolism" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Blocking of the PULMONARY ARTERY or one of its branches by an EMBOLUS.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000722",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Cerebr\u00E1ln\u00ED infarkt" , "@language" : "cs" } ,        { "@value" : "Cerebral infarction" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "The formation of an area of NECROSIS in the CEREBRUM caused by an insufficiency of arterial or venous blood flow. Infarcts of the cerebrum are generally classified by hemisphere (i.e., left vs. right), lobe (e.g., frontal lobe infarction), arterial distribution (e.g., INFARCTION, ANTERIOR CEREBRAL ARTERY), and etiology (e.g., embolic infarction).     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000408",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Angina, unstable" , "@language" : "en" } ,        { "@value" : "Nestabiln\u00ED angina pectoris" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Precordial pain at rest, which may precede a MYOCARDIAL INFARCTION.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000003109",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Varicose ulcer" , "@language" : "en" } ,        { "@value" : "B\u00E9rcov\u00E9 v\u0159edy" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Skin breakdown or ulceration caused by varicose veins in which there is too much hydrostatic pressure in the superficial venous system of the leg. Venous hypertension leads to increased pressure in the capillary bed, transudation of fluid and proteins into the interstitial space, altering blood flow and supply of nutrients to the skin and subcutaneous tissues, and eventual ulceration.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000000790",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Cicatrix" , "@language" : "en" } ,        { "@value" : "Jizva" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "The fibrous tissue that replaces normal tissue during the process of WOUND HEALING.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000003062",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "V\u0159ed" , "@language" : "cs" } ,        { "@value" : "Ulcer" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A lesion on the surface of the skin or a mucous surface, produced by the sloughing of inflammatory necrotic tissue.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000003117",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Venous insufficiency" , "@language" : "en" } ,        { "@value" : "\u017Diln\u00ED insuficience" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Impaired venous blood flow or venous return (venous stasis), usually caused by inadequate venous valves. Venous insufficiency often occurs in the legs, and is associated with EDEMA and sometimes with VENOUS STASIS ULCERS at the ankle.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000001460",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Hematoma" , "@language" : "en" } ,        { "@value" : "Hematom" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A collection of blood outside the BLOOD VESSELS. Hematoma can be localized in an organ, space, or tissue.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000008556",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PhysiologicEffect" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Decreased Coagulation Factor Activity" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000009960",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MechanismOfAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Antithrombin Activators" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000009963",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MechanismOfAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Thrombin Inhibitors" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0001380",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Anticoagulants" , "@language" : "en" } ,        { "@value" : "Antikoagulancia" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Agents that prevent clotting.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0008434",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.ope