e" : [ { "@value" : "Tromboembolie" , "@language" : "cs" } ,        { "@value" : "Thromboembolism" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Obstruction of a blood vessel (embolism) by a blood clot (THROMBUS) in the blood stream.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002135",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Neoplasms, hormone-dependent" , "@language" : "en" } ,        { "@value" : "N\u00E1dory z\u00E1visl\u00E9 na hormonech" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Certain tumors that 1, arise in organs that are normally dependent on specific hormones and 2, are stimulated or caused to regress by manipulation of the endocrine environment.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002500",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Prostatic neoplasms" , "@language" : "en" } ,        { "@value" : "Prostata - n\u00E1dory" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Tumors or cancer of the PROSTATE.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000001636",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Hypogonadism" , "@language" : "en" } ,        { "@value" : "Hypogonadismus" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Condition resulting from deficient gonadal functions, such as GAMETOGENESIS and the production of GONADAL STEROID HORMONES. It is characterized by delay in GROWTH, germ cell maturation, and development of secondary sex characteristics. Hypogonadism can be due to a deficiency of GONADOTROPINS (hypogonadotropic hypogonadism) or due to primary gonadal failure (hypergonadotropic hypogonadism).     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000003303",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Osteopor\u00F3za postmenopauz\u00E1ln\u00ED" , "@language" : "cs" } ,        { "@value" : "Osteoporosis, postmenopausal" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Metabolic disorder associated with fractures of the femoral neck, vertebrae, and distal forearm. It occurs commonly in women within 15-20 years after menopause, and is caused by factors associated with menopause including estrogen deficiency.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000001993",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Menoragie" , "@language" : "cs" } ,        { "@value" : "Menorrhagia" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Excessive uterine bleeding during MENSTRUATION.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000001992",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Menopauza p\u0159ed\u010Dasn\u00E1" , "@language" : "cs" } ,        { "@value" : "Menopause, premature" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "The premature cessation of menses (MENSTRUATION) when the last menstrual period occurs in a woman under the age of 40. It is due to the depletion of OVARIAN FOLLICLES. Premature MENOPAUSE can be caused by diseases; OVARIECTOMY; RADIATION; chemicals; and chromosomal abnormalities.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000003452",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Prim\u00E1rn\u00ED ovari\u00E1ln\u00ED insuficience" , "@language" : "cs" } ,        { "@value" : "Primary ovarian insufficiency" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Cessation of ovarian function after MENARCHE but before the age of 40, without or with OVARIAN FOLLICLE depletion. It is characterized by the presence of OLIGOMENORRHEA or AMENORRHEA, elevated GONADOTROPINS, and low ESTRADIOL levels. It is a state of female HYPERGONADOTROPIC HYPOGONADISM. Etiologies include genetic defects, autoimmune processes, chemotherapy, radiation, and infections.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002464",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Porphyrias" , "@language" : "en" } ,        { "@value" : "Porfyrie" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "A diverse group of metabolic diseases characterized by errors in the biosynthetic pathway of HEME in the LIVER, the BONE MARROW, or both. They are classified by the deficiency of specific enzymes, the tissue site of enzyme defect, or the clinical features that include neurological (acute) or cutaneous (skin lesions). Porphyrias can be hereditary or acquired as a result of toxicity to the hepatic or erythropoietic marrow tissues.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000008311",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PhysiologicEffect" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Bone Formation Stimulation" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000009902",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PhysiologicEffect" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Vaginal Function Alteration" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000000100",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MechanismOfAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Estrogen Receptor Agonists" , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/ingredient/M0007772",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/Ingredient" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Estradiol" , "@language" : "en" } ,        { "@value" : "Estradiol" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPharmacologicalAction" : [ "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0007795" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMedicinalProduct" : [ "http://linked.opendata.cz/resource/sukl/medicinal-product/ZOELY-2-5-MG-1-5-MG" ,        "http://linked.opendata.cz/resource/sukl/medicinal-product/IOA-2-5-MG-1-5MG" ,        "http://linked.opendata.cz/resource/sukl/medicinal-product/OESTROGEL" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Generally refers to the 17-beta-isomer of estradiol, an aromatized C18 steroid with hydroxyl group at 3-beta- and 17-beta-position. Estradiol-17-beta is the most potent form of mammalian estrogenic steroids. In humans, it is produced primarily by the cyclic ovaries and the PLACENTA. It is also produced by the adipose tissue of men and postmenopausal women. The 17-alpha-isomer of estradiol binds weakly to estrogen receptors (RECEPTORS, ESTROGEN) and exhibits little estrogenic activity in estrogen-responsive tissues. Various isomers can be synthesized.     " , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasMechanismOfAction" : [ "http://linked.opendata.cz/resource/ndfrt/mechanism-of-action/N0000000100" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/mayTreat" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000001636" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000001993" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000001992" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000003452" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000002500" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000000612" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000003943" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/mayPrevent" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000003303" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/contraindicatedWith" : [ "http://linked.opendata.cz/resource/ndfrt/disease/N0000002464" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000002935" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000000612" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000002934" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000002135" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000000999" ,        "http://linked.opendata.cz/resource/ndfrt/disease/N0000010195" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasPhysiologicEffect" : [ "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000008311" ,        "http://linked.opendata.cz/resource/ndfrt/physiologic-effect/N0000009902" ] } ,    { "@id": "http://linked.opendata.cz/resource/sukl/medicinal-product/IOA-2-5-MG-1-5MG",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MedicinalProduct" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "IOA 2,5 MG/1,5MG" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasATCConcept" : [ "http://linked.opendata.cz/resource/ATC/G03AA14" ] } ,    { "@id": "http://linked.opendata.cz/resource/sukl/medicinal-product/ZOELY-2-5-MG-1-5-MG",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MedicinalProduct" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "ZOELY 2,5 MG/1,5 MG" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasATCConcept" : [ "http://linked.opendata.cz/resource/ATC/G03AA14" ] } ,    { "@id": "http://linked.opendata.cz/resource/drug-encyclopedia/pharmacological-action/M0007795",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/PharmacologicalAction" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Estrogeny" , "@language" : "cs" } ,        { "@value" : "Estrogens" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Compounds that interact with ESTROGEN RECEPTORS in target tissues to bring about the effects similar to those of ESTRADIOL. Estrogens stimulate the female reproductive organs, and the development of secondary female SEX CHARACTERISTICS. Estrogenic chemicals include natural, synthetic, steroidal, or non-steroidal compounds.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/sukl/medicinal-product/OESTROGEL",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/MedicinalProduct" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "OESTROGEL" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/hasATCConcept" : [ "http://linked.opendata.cz/resource/ATC/G03CA03" ] }] }