"@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Cystic fibrosis" , "@language" : "en" } ,        { "@value" : "Cystick\u00E1 fibr\u00F3za" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "An autosomal recessive genetic disease of the EXOCRINE GLANDS. It is caused by mutations in the gene encoding the CYSTIC FIBROSIS TRANSMEMBRANE CONDUCTANCE REGULATOR expressed in several organs including the LUNG, the PANCREAS, the BILIARY SYSTEM, and the SWEAT GLANDS. Cystic fibrosis is characterized by epithelial secretory dysfunction associated with ductal obstruction resulting in AIRWAY OBSTRUCTION; chronic RESPIRATORY INFECTIONS; PANCREATIC INSUFFICIENCY; maldigestion; salt depletion; and HEAT PROSTRATION.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002432",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Pneumonia" , "@language" : "en" } ,        { "@value" : "Pneumonie" , "@language" : "cs" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Inflammation of any part, segment or lobe, of the lung parenchyma.     " , "@language" : "en" } ] } ,    { "@id": "http://linked.opendata.cz/resource/ndfrt/disease/N0000002265",      "@type" : [ "http://linked.opendata.cz/ontology/drug-encyclopedia/DiseaseOrFinding" ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/title" : [ { "@value" : "Otitis externa" , "@language" : "cs" } ,        { "@value" : "Otitis externa" , "@language" : "en" } ] ,      "http://linked.opendata.cz/ontology/drug-encyclopedia/description" : [ { "@value" : "Inflammation of the OUTER EAR including the external EAR CANAL, cartilages of th