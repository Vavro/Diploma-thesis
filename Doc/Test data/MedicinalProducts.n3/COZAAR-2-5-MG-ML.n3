    ns1:hasRegistrationState "http://linked.opendata.cz/resource/sukl/registration-state/R" ;
    ns1:hasRouteOfAdministration "Perorální podání"@cs ;
    ns1:hasSPCDocument "http://linked.opendata.cz/resource/sukl/spc/document/SPC159273_doc" ;
    ns1:hasStrength "2.5MG/ML"^^xsd:string ;
    ns1:hasTitleSupplement "POR PSU LQF 1X500MG"@cs ;
    ns1:title "COZAAR 2,5 MG/ML"@cs .

